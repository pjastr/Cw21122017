<Window x:Class="WpfApplication6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="Przycisk" Content="Dodaj" HorizontalAlignment="Left" Margin="243,206,0,0" VerticalAlignment="Top" Width="100" Padding="10" BorderThickness="10" Click="Przycisk_Click"/>
        <TextBox x:Name="Liczba1" HorizontalAlignment="Left" Height="23" Margin="145,56,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Liczba2" HorizontalAlignment="Left" Height="23" Margin="145,109,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Pierwsza liczba" HorizontalAlignment="Left" Margin="41,56,0,0" VerticalAlignment="Top"/>
        <Label Content="Druga liczba" HorizontalAlignment="Left" Margin="41,109,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.443,0.319"/>
        <Label x:Name="Wynik" Content="Wynik" HorizontalAlignment="Left" Margin="117,206,0,0" VerticalAlignment="Top"/>
        
        <Button x:Name="PrzyciskCzas" Content="Czas" HorizontalAlignment="Left" Margin="399,96,0,0" VerticalAlignment="Top" Width="75" MouseEnter="PrzyciskCzas_MouseEnter" MouseLeave="PrzyciskCzas_MouseLeave"/>

    </Grid>
</Window>
